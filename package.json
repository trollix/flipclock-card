  "keywords": [
    "home-assistant",
    "homeassistant",
    "hass",
    "automation",
    "lovelace",
    "custom-cards"
  ],
  "main": "src/main.js",
  "module": "src/main.js",
  "repository": "https://github.com/trollix/flipclock-card.git",
  "author": "trollix",
  "license": "MIT",
  "dependencies": {
    "@formatjs/ecma402-abstract": "^1.14.3",
    "custom-card-helpers": "^1.9.0",
    "flipclock": "^0.10.5",
    "lit-element": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@lopatnov/rollup-plugin-uglify": "^2.1.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-serve": "^2.0.2",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "rollup -c --watch --bundleConfigAsCjs",
    "build": "npm run lint && npm run rollup",
    "release-patch": "npm version patch --no-git-tag-version && npm run release",
    "release-minor": "npm version minor --no-git-tag-version && npm run release",
    "release-major": "npm version major --no-git-tag-version && npm run release",
    "release": "npm run build",
    "commit": "git add -A && git commit -m $npm_package_version",
    "lint": "eslint src/*.ts",
    "rollup": "rollup -c --bundleConfigAsCjs",
    "pretty": "prettier --write \"./**/*.{js,ts}\""
